<UserControl x:Class="CmisSync.AboutWPF"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="248" d:DesignWidth="640">
    <Grid>
        <Canvas x:Name="canvas" HorizontalAlignment="Left" Height="248" Margin="0,0,0,0" VerticalAlignment="Top" Width="620">
            <Image x:Name="image" HorizontalAlignment="Left" Height="260" VerticalAlignment="Top" Width="640"/>
            <Label x:Name="version" Content="version" HorizontalAlignment="Left" Canvas.Left="289" Canvas.Top="92" FontSize="11" VerticalAlignment="Top"/>
            <TextBlock x:Name="credits" HorizontalAlignment="Left" Canvas.Left="294" Canvas.Top="142" FontSize="11" TextWrapping="Wrap" Text="credits" VerticalAlignment="Top" Width="318"/>
        </Canvas>
    </Grid>
</UserControl>
